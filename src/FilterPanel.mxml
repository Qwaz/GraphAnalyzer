<?xml version="1.0" encoding="utf-8"?>
<s:Panel xmlns:fx="http://ns.adobe.com/mxml/2009" 
			   xmlns:s="library://ns.adobe.com/flex/spark" 
			   xmlns:mx="library://ns.adobe.com/flex/mx" xmlns:local="*">
	<fx:Script>
		<![CDATA[
			import mx.collections.ArrayList;
			import flash.utils.getQualifiedClassName;
			import filters.Filter;
			import filters.ConditionFilter;
			import filters.TimeFilter;
			
			[Bindable]
			private var filterList:ArrayList = new ArrayList([
			{label:'작다', filter:ConditionFilter },
			{label:'NOR', filter:TimeFilter },
			]);
		]]>
	</fx:Script>
	<mx:VDividedBox width="100%">
		<s:List width="100%" height="70%">
			
		</s:List>
		<s:List width="100%" height="30%" dataProvider="{filterList}">
		</s:List>
	</mx:VDividedBox>
</s:Panel>